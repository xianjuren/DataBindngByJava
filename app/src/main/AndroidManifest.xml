<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="databingdingbyjava.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".basic.BasicActivity"/>
        <activity android:name=".customBinding.CustomActivity"/>
        <activity android:name=".include.IncludeActivity"/>
        <activity android:name=".collection.CollectionActivity"/>
        <activity android:name=".resource.ResourceActivity"/>
        <activity android:name=".observable.ObservableActivity"/>
        <activity android:name=".viewById.ViewIdActivity"/>
        <activity android:name=".viewStub.ViewStubActivity"/>
        <activity android:name=".recyclerView.RecyclerViewActivity"/>
        <activity android:name=".attributeSetter.AttributeSettersActivity"/>
        <activity android:name=".convert.ConvertActivity"/>

    </application>

</manifest>